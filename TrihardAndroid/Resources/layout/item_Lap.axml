            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/lapKm"
                android:gravity="center"
                android:textColor="#ffe5a109"
                android:layout_weight="0.8"
                android:textSize="12dp"
                android:text="lapKm" />
            <TextView
