                                        android:gravity="center_vertical">
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:id="@+id/lap"
                                            android:textColor="#ffffffff"
                                            android:singleLine="true"
                                            android:layout_weight="0.6"
                                            android:ellipsize="end"
                                            android:gravity="center"
                                            android:textSize="12dp"
                                            android:text="Lap" />
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:id="@+id/elapsedTime"
                                            android:textColor="#ffffffff"
                                            android:singleLine="true"
                                            android:layout_weight="1"
                                            android:ellipsize="end"
                                            android:gravity="center"
                                            android:textSize="12dp"
                                            android:text="Lap Time" />
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:id="@+id/avgHr"
                                            android:textColor="#ffffffff"
                                            android:singleLine="true"
                                            android:layout_weight="0.8"
                                            android:ellipsize="end"
                                            android:gravity="center"
                                            android:text="Avg.Hr"
                                            android:textSize="12dp" />
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:id="@+id/avgPower"
                                            android:textColor="#ffffffff"
                                            android:singleLine="true"
                                            android:layout_weight="1"
                                            android:ellipsize="end"
                                            android:gravity="center"
                                            android:text="Avg.Power"
                                            android:textSize="12dp" />
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:id="@+id/time"
                                            android:textColor="#ffffffff"
                                            android:singleLine="true"
                                            android:layout_weight="1"
                                            android:ellipsize="end"
                                            android:gravity="center"
                                            android:textSize="12dp"
                                            android:text="Time" />
                                    </LinearLayout>
                                </LinearLayout>
                            </RelativeLayout>
                            <ListView
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:id="@+id/listLaps"
                                android:nestedScrollingEnabled="true"
                                android:padding="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:divider="@null"
                                android:dividerHeight="0dp" />
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout xmlns:tools="http://schemas.android.com/tools"
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout3"
                tools:visibility="visible">
                <Button
                    android:text="ADJUST TRAINING"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:id="@+id/ActionAdjustTrainning"
                    android:background="#ffe5a109"
                    android:textColor="#ffffffff"
                    android:textSize="12dp"
                    android:layout_margin="20dp"
                    android:textStyle="bold"
                    tools:visibility="visible" />
            </LinearLayout>
            <RelativeLayout
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:id="@+id/ActionLocation"
                android:background="#ff1e1e1e"
                android:layout_marginBottom="1dp"
                android:tag="1">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout9"
                    android:gravity="center_vertical">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/linearLayout10"
                        android:layout_marginLeft="10dp">
                        <ImageView
                            android:src="@drawable/icon_map_pin"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:id="@+id/imageView1"
                            android:padding="3dp" />
                    </LinearLayout>
                    <TextView
                        android:text="LOCATION"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView2"
                        android:textStyle="bold"
                        android:textSize="12dp"
                        android:layout_marginLeft="10dp"
                        android:textColor="#ffffffff"
                        android:layout_weight="1" />
                    <ImageView
                        android:src="@drawable/icon_right"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:id="@+id/btnRun"
                        android:layout_marginRight="20dp" />
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/viewRun"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout12"
                    android:layout_marginBottom="20dp"
                    android:gravity="center_horizontal">
                    <TextView
                        android:text="COMMENTS"
                        android:id="@+id/lblCommentTitle"
                        android:textStyle="bold"
                        android:textColor="#ffffffff"
                        android:textSize="13dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:layout_marginRight="30dp" />
                    <ImageView
                        android:src="@drawable/icon_add_comment"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:id="@+id/ActionAddComment" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout17"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#ff1e1e1e" />
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/contentComment"
                    android:layout_marginBottom="30dp" />
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:id="@+id/footerView" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
